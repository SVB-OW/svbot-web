{
	"name": "svbot-web",
	"version": "1.2.0",
	"license": "GPL-3.0-or-later",
	"scripts": {
		"dev": "npx concurrently \"npm run dev:client\" \"npm run dev:server\"",
		"dev:client": "nuxt dev",
		"dev:server": "npx nodemon api.js",
		"build": "nuxt build",
		"start": "npx concurrently \"node ./.output/server/index.mjs\" \"node api.js\"",
		"generate": "nuxt generate",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"format": "prettier --check \"./**/*.{vue,ts,js}\" --ignore-path .gitignore",
		"format:fix": "prettier --write \"./**/*.{vue,ts,js}\" --ignore-path .gitignore",
		"typecheck": "nuxi typecheck"
	},
	"dependencies": {
		"@pinia/nuxt": "^0.5.1",
		"cors": "^2.8.5",
		"dotenv": "^16.0.3",
		"express": "^4.17.1",
		"express-ws": "^5.0.2",
		"mongodb": "^6.5.0",
		"nuxt": "^3.11.1",
		"pinia": "^2.1.7",
		"socket.io": "^4.6.1",
		"socket.io-client": "^4.3.2",
		"vue": "^3.4.21",
		"vue-router": "^4.3.0"
	},
	"devDependencies": {
		"@nuxt/eslint": "^0.3.13",
		"@nuxtjs/eslint-config-typescript": "^12.1.0",
		"eslint": "^9.3.0",
		"prettier": "^3.2.5",
		"typescript": "^5.4.5",
		"vue-tsc": "^1"
	}
}
