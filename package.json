{
	"name": "svbot-web",
	"version": "1.2.0",
	"license": "GPL-3.0-or-later",
	"scripts": {
		"dev": "npx concurrently \"npm run dev:client\" \"npm run dev:server\"",
		"dev:client": "nuxt",
		"dev:server": "npx nodemon api.js",
		"build": "nuxt build",
		"start": "npx concurrently \"nuxt start\" \"node api.js\"",
		"generate": "nuxt generate",
		"lint-fix": "eslint . --fix",
		"format": "prettier --check \"./**/*.{vue,ts,js}\" --ignore-path .gitignore",
		"format-fix": "prettier --write \"./**/*.{vue,ts,js}\" --ignore-path .gitignore",
		"lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
		"lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
		"lint:prettier": "prettier --check .",
		"lint": "yarn lint:js && yarn lint:style && yarn lint:prettier",
		"lintfix": "prettier --write --list-different . && yarn lint:js --fix && yarn lint:style --fix"
	},
	"dependencies": {
		"@nuxt/typescript-build": "^2.1.0",
		"core-js": "^3.25.3",
		"cors": "^2.8.5",
		"dotenv": "^16.0.3",
		"express": "^4.17.1",
		"express-ws": "^5.0.2",
		"mongodb": "^4.11.0",
		"nuxt": "^2.15.8",
		"socket.io": "^4.3.2",
		"socket.io-client": "^4.3.2",
		"vue": "^2.7.10",
		"vue-server-renderer": "^2.7.10",
		"vue-template-compiler": "^2.7.10"
	},
	"devDependencies": {
		"@babel/eslint-parser": "^7.19.1",
		"@nuxt/types": "^2.15.8",
		"@nuxt/typescript-build": "^2.1.0",
		"@nuxtjs/eslint-config-typescript": "^11.0.0",
		"@nuxtjs/eslint-module": "^3.1.0",
		"@types/mongodb": "^4.0.7",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"eslint": "^8.24.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-nuxt": "^4.0.0",
		"eslint-plugin-vue": "^9.5.1",
		"postcss-html": "^1.5.0",
		"prettier": "^2.7.1",
		"stylelint": "^14.13.0",
		"stylelint-config-prettier": "^9.0.3",
		"stylelint-config-recommended-vue": "^1.4.0",
		"stylelint-config-standard": "^28.0.0"
	}
}
