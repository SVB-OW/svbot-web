{
  "name": "svbot-web",
  "version": "1.3.0",
  "license": "GPL-3.0-or-later",
  "type": "module",
  "packageManager": "pnpm@9.1.4+sha512.9df9cf27c91715646c7d675d1c9c8e41f6fce88246f1318c1aa6a1ed1aeb3c4f032fcdf4ba63cc69c4fe6d634279176b5358727d8f2cc1e65b65f43ce2f8bfb0",
  "scripts": {
    "dev": "npx concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "nuxt dev",
    "dev:server": "npx nodemon api.js",
    "build": "nuxt build",
    "start": "npx concurrently \"node ./.output/server/index.mjs\" \"node api.js\"",
    "generate": "nuxt generate",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:style": "stylelint \"**/*.{css,vue}\" --ignore-path .gitignore",
    "lint:style:fix": "stylelint \"**/*.{css,vue}\" --ignore-path .gitignore --fix",
    "format": "prettier --check \"./**/*.{vue,ts,js}\" --ignore-path .gitignore",
    "format:fix": "prettier --write \"./**/*.{vue,ts,js}\" --ignore-path .gitignore",
    "typecheck": "nuxi typecheck",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.5.1",
    "@unhead/vue": "^1.9.12",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-ws": "^5.0.2",
    "mongodb": "^6.7.0",
    "nuxt": "^3.11.2",
    "pinia": "^2.1.7",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@nuxt/eslint": "^0.3.13",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/stylelint-module": "^5.2.0",
    "@storybook/addon-essentials": "^8.1.5",
    "@storybook/addon-interactions": "^8.1.5",
    "@storybook/addon-links": "^8.1.5",
    "@storybook/blocks": "^8.1.5",
    "@storybook/test": "^8.1.5",
    "@storybook/vue3": "^8.1.5",
    "@storybook/vue3-vite": "^8.1.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "eslint": "^9.4.0",
    "eslint-plugin-storybook": "^0.8.0",
    "prettier": "^3.3.0",
    "storybook": "^8.1.5",
    "stylelint": "^16.6.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.12",
    "vue-tsc": "^2.0.19"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
